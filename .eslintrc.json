{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "eqeqeq": "error",
    "indent": ["error", 2],
    "jsx-quotes": ["error", "prefer-double"],
    "no-var": "error",
    "no-extra-semi": "error",
    "no-unused-vars": "error",
    "prefer-template": "error",
    "quotes": ["error", "double", { "allowTemplateLiterals": true }]
  }
}
